# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page

#index page
GET     /                           controllers.HomeController.index
GET  	/add						controllers.HomeController.add(request: Request)

#static routes
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

#Project pages
GET 	/xmlAttributeToolForm		controllers.HomeController.openAttributeToolForm(request: Request)

+ nocsrf
POST	/attributeTool				controllers.HomeController.attributeTool(request: Request)

